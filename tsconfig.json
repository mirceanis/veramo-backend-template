{
  "compilerOptions": {
    "target": "ES2018",
    "module": "ESNext",
    "lib": ["dom", "es6", "es2015.promise"],
    "outDir": "lib",
    "strict": true,
    "types": ["jest"],
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "skipLibCheck": false,
    "forceConsistentCasingInFileNames": true
  }
}
